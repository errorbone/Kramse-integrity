LOGIN_URL=http://93.90.115.26:8080/users/signup/
YOUR_USER='henningg'
YOUR_EMAIL='username@aurehoej.net'
YOUR_FIRST_NAME='Bib'
YOUR_LAST_NAME='Bibbib'
YOUR_PASS1='sli!!w1ord'
YOUR_PASS2='sli!!w1ord'
COOKIES=cookies.txt
CURL_BIN="curl -s -c $COOKIES -b $COOKIES -e $LOGIN_URL"

echo -n "Django Auth: get csrftoken ..."
$CURL_BIN $LOGIN_URL > /dev/null
DJANGO_TOKEN="csrfmiddlewaretoken=$(grep csrftoken $COOKIES | sed 's/^.*csrftoken\s*//')"

echo -n " perform login ..."
$CURL_BIN \
    -d "$DJANGO_TOKEN&id_username=$YOUR_USER&id_email=$YOUR_EMAIL&id_first_name=$YOUR_FIRST_NAME&id_last_name=$YOUR_LAST_NAME&password1=$YOUR_PASS&password2=$YOUR_PASS" \
    -X POST $LOGIN_URL

echo " logout"
rm $COOKIES







#/users/signup/&username=abc&email=abcde%40aurehoej.net&first_name=agr&last_name=ffbc&password1=henrik42&password2=henrik42